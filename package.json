{
  "name": "imagemin",
  "version": "1.5.1",
  "private": true,
  "description": "Minify PNG, JPEG and GIF images with imagemin",
  "license": "MIT",
  "repository": "1000ch/atom-imagemin",
  "author": {
    "name": "Shogo Sensui",
    "email": "shogo.sensui@gmail.com",
    "url": "https://github.com/1000ch"
  },
  "scripts": {
    "test": "apm test",
    "pretest": "eslint ."
  },
  "keywords": [
    "imagemin",
    "image",
    "img",
    "png",
    "jpg",
    "gif",
    "minify"
  ],
  "configSchema": {
    "jpegQuality": {
      "title": "JPEG Quality",
      "type": "number",
      "default": 80,
      "minimum": 0,
      "maximum": 100
    },
    "progressive": {
      "title": "Progressive",
      "type": "boolean",
      "default": true
    },
    "pngQuality": {
      "title": "PNG Quality",
      "type": "number",
      "default": 80,
      "minimum": 0,
      "maximum": 100
    },
    "interlace": {
      "title": "Interlace",
      "type": "boolean",
      "default": true
    }
  },
  "dependencies": {
    "filesize": "^3.3.0",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegoptim": "^5.0.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.0.0",
    "pify": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "tempy": "^0.1.0"
  }
}
